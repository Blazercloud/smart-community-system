<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.community.admin.mapper.AdminMapper">
    <select id="findByUsername" parameterType="string" resultType="com.neusoft.community.admin.entity.Admin">
        SELECT * FROM admin WHERE username = #{username} LIMIT 1
    </select>
</mapper>